{
    "$schema": "https://www.krakend.io/schema/v3.json",
    "version": 3,
    "name": "DevOpsBootCamp",
    "timeout": "3000ms",
    "cache_ttl": "300s",
    "output_encoding": "json",
    "port": 3890,
    "debug_endpoint": true,
    "echo_endpoint": true,
    "sequential_start": true,

    "endpoints": [
    { 
        "endpoint": "/v1/secure-endpoint",
        "method": "GET",
        
        "extra_config": {
            "auth/validator": {
                "alg": "RS256",
                "jwk_url": "https://devops-bootcamp.on.qriarlabs.com/realms/krakend/protocol/openid-connect/certs",
                "disable_jwk_security": true
            }
        },
   
        "output_encoding": "json",
        "backend": [
          {
            "url_pattern": "/bin/1ad0702c-64f5-49ca-a12d-009659897249",
            "encoding": "json",
            "sd": "static",
            "method": "GET",
            "host": [
              "https://mockbin.org"
            ],
            "disable_host_sanitize": false
          }
        ]
      }
  
  
    ]
  }